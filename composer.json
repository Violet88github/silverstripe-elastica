{
    "name": "violet88/silverstripe-elastica",
    "description": "Module for Silverstripe to use Elastic Search",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "ruflin/elastica": "^7.2",
        "symbiote/silverstripe-gridfieldextensions": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Violet88\\Elastica\\": "src/",
            "Violet88\\Elastica\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
